<cfscript>year=attributes.year;golden=(year mod 19)+1;century=Int(year/100)+1;correction1=Int(3*century/4)-12;correction2=Int((8*century+5)/25)-5;sunday=Int(5*year/4)-correction1-10;epact=(11*golden+20+correction2-correction1) mod 30;if((epact eq 25 and golden gt 11) or (epact eq 24)){epact=epact+1;};n=44-epact;if(n lt 21){n=n+30;};offset=n+7-((sunday+n) mod 7);holidayInstance=DateAdd('d', -1, DateAdd('d',offset,CreateDate(year,3,1)));caller.startDateTime = holidayInstance;</cfscript>
